
<!-- Begin pattern: breadcrumbs //-->

<div class="container">
  <div class="row">
    <div class="col-lg-12 content">
      <ul class="breadcrumbs">
      {{#if parents}}{{#parents}}<li><a href="{{url}}">{{title}}</a></li>{{/parents}}{{else}}<li><a href="/">Parent1</a></li><li><a href="/parent2/">Parent 2</a></li>{{/if}}<li>{{default current.title "[current.title]"}}</li>
      </ul>
    </div>
  </div>
</div>

<!-- End pattern: breadcrumbs //-->
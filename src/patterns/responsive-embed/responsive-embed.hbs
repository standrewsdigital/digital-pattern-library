<!-- Begin pattern: responsive-embed //-->
<div class="container">
    <div class="row">
        {{#if column1}}
            <div class="{{column1}}">
                <div class="embed-responsive embed-responsive-{{ratio}} google-map">
                    {{{iframe}}}
                </div>
            </div>
            <div class="{{column2}}">
                {{{text}}}
            </div>        
        {{else}}
            <div class="col-sm-12">
                <div class="embed-responsive embed-responsive-{{ratio}} google-map">
                    {{{iframe}}}
                </div>
            </div>
        {{/if}}
    </div>
</div>
<!-- End pattern: responsive-embed //-->
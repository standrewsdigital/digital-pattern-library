                <div class="tab-content">
                    <div class="tab-pane fade active in" id="tab-1" role="tabpanel" style="padding:0;border:0;">
                    
<section class="hero-section {{test}}">
    <figure>
    <picture>
      <source srcset="{{asset bg-image}}hero1/1920x600.jpg" media="(min-width: 640px)">
      <img src="{{asset bg-image}}hero1/750x500.jpg" alt="">
    </picture>
    <figcaption>Caption is optional</figcaption>
    </figure>
    <div class="container">
        <div class="hero-section__content">
            <h2 class="hero-section__heading">{{default heading "[heading]"}}</h2>
            <p class="hero-section__intro">{{default paragraph "[paragraph]"}}</p>
            {{#if action-text}}
            <a class="{{button-style}}" href="{{default action-url "[action-url]"}}">{{default action-text "[action-text]"}}</a>
            {{/if}}
            {{#if action-text-2}}
            <a class="{{button-style}}" href="{{default action-url "[action-url]"}}">{{default action-text-2 "[action-text-2]"}}</a>
            {{/if}}
        </div>
    </div>
</section>

</div>

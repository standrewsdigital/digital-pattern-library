

<!-- Begin pattern: masonry-grid //-->

<div class="masonry-grid">
  
  <div class="container">

	<div class="row">

	  <div class="col-sm-6 col-md-4">
			{{> tile tile-1 }}
		</div>

		<div class="col-sm-6 col-md-4">
			{{> tile tile-2}}
	  </div>

	  <div class="col-sm-6 col-md-4">
			{{> tile tile-3}}
		</div>

		<div class="col-sm-6 col-md-4">
			{{> tile tile-4}}
	  </div>

	  <div class="col-sm-6 col-md-4">
			{{> tile tile-5}}
		</div>

		<div class="col-sm-6 col-md-4">
			{{> tile tile-6}}
	  </div>

	</div>  <!-- // End .row -->

  </div> <!-- // End .container -->

</div>  <!-- // End .masonry-grid -->

<!-- End pattern: masonry-grid //-->
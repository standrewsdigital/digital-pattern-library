<!-- Begin pattern: panel //-->
{{#each panels}}
<div class="{{column-size}}">
{{#if image}}
	<div class="panel">
		<div class="panel-image">
        	<img src="{{image.url}}" alt="{{image.alt}}">
        </div>
{{else}}
	<div class="panel {{color}}">
		<div class="panel-heading">
			<h3 class="panel-title"><a href="#">{{title}}</a></h3>
		</div>
{{/if}}
		<div class="panel-body">
			{{{description}}}
		</div>
	</div>
</div>
{{/each}}
<!-- End pattern: panel //-->
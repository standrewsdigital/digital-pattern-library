 
 <!-- Begin pattern: navbox //-->

	<div class="navbox">
	    <div class="navbox-image">
      {{#if image}}
      <img src="{{asset image.url}}" alt="{{image.alt}}"></div>
      {{else}}
      <img src="[path/to/image.ext]" alt="[alt]"></div>
      {{/if}}
	    <div class="navbox-title">
	        <div class="navbox-chevron"><img src="{{asset "images/furniture/chevron@2x.png"}}" alt="chevron" /></div>
	        <p><a href="{{default url "[url]"}}">{{default title "[title]"}}</a></p>
		</div>
	</div>

<!-- End pattern: navbox //-->

<!-- Begin pattern: paired-values-list //-->
<dl class="paired-values-list">
    {{#each list}}
    <dt class="paired-values-list__name{{#if item-selected}} selected{{/if}}">{{list-name}}</dt>
    <dd class="paired-values-list__value{{#if item-selected}} selected{{/if}}">{{list-value}}</dd>
    {{/each}}
</dl>
<!-- End pattern: paired-values-list //-->

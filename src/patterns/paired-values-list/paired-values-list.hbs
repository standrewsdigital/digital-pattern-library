{{#if before-pattern ~}}
{{{before-pattern}}}
{{/if ~}}
<!-- Begin pattern: paired-values-list //-->
<dl class="paired-values-list{{list-classes}}">
    {{#each list}}
    <dt class="paired-values-list__name{{#if name-selected}} font-weight--bold{{/if}}">{{list-name}}</dt>
    <dd class="paired-values-list__value{{#if value-selected}} font-weight--bold{{/if}}">{{{list-value}}}</dd>
    {{/each}}
</dl>
<!-- End pattern: paired-values-list //-->
{{#if after-pattern}}{{{after-pattern}}}{{/if}}
